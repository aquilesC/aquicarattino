root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 4
indent_style = space
insert_final_newline = false
max_line_length = 120
tab_width = 4

[*.css]
indent_size = 2
tab_width = 2

[{*.pyw,*.py}]
indent_size = 4
tab_width = 4

[{*.sht,*.html,*.shtm,*.shtml,*.htm}]
indent_size = 2
tab_width = 2
